{
  "metadata": [
    {
      "src": [
        {
          "files": ["Math.Lib/Math.Lib.csproj"],
          "cwd": "."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["docs/**.md"],
        "dest": "."
      }
    ],
    "resource": [],
    "globalMetadata": {
      "_appTitle": "MyMath Docs"
    },
    "overwrite": [],
    "dest": "_site",
    "template": ["default"],
    "markdownEngineName": "markdig"
  }
}
